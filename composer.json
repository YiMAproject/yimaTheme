{
    "name": "rayamedia/yima-theme",
    "type": "yima-core-module",
    "description": "Professional UI layout system, Simple Powerfull, Maintainable, Extendable.",
    "keywords": [
        "theme",
        "template",
        "layout",
        "ui",
        "zf2 module",
        "zf2",
        "yima",
        "raya media"
    ],
    "authors": [
        {
            "name": "Payam Naderi",
            "email": "naderi.payam@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rayamedia/yima-plugin-installer": "dev-master"
    },
    "extra": {
        "yima-plugin-name": "yimaTheme"
    },
    "autoload" : {
        "psr-0" : {
            "yimaTheme\\" : "src/"
        },
        "classmap" : [
            "./Module.php"
        ]
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.raya-media.com/"
        }
    ]
}
